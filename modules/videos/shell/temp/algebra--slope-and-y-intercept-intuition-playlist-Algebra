                <input type="hidden" name="entity_key" value="ag1raGFuZXhlcmNpc2VzchELEghGZWVkYmFjaxiziuEEDA" />
                <div class="question" data-question_id="9979187">
                    
                        <a class="question_link" href="/video/algebra--slope-and-y-intercept-intuition?playlist=Algebra&qa_expand_id=9979187#q_9979187" onclick="return false;">
                            <span>find the slope of (4,7)and (-6,7)   m=_________________________</span>
                        </a>
                    
                    <span
                    
                        class="question_content_urlized" style="display:none;"
                    
                    >find the slope of (4,7)and (-6,7)   m=_________________________</span>
                </div>

                
                    <div class="question_answer_count
                    
                    ">
                        <table><tr>
                        <td style="vertical-align: middle;">
                            <a href="/video/algebra--slope-and-y-intercept-intuition?playlist=Algebra&qa_expand_id=9979187#q_9979187" onclick="return false;">3 answers</a>
